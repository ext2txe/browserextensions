{
  "manifest_version": 3,
  "name": "Advanced History Search",
  "version": "1.0.0",
  "description": "Powerful history search with pattern matching, filters, and exports",
  "permissions": [
    "history",
    "storage",
    "sidePanel"
  ],
  "side_panel": {
    "default_path": "sidebar/sidebar.html"
  },
  "action": {
    "default_icon": {
      "16": "icons/icon-16.png",
      "32": "icons/icon-32.png"
    },
    "default_title": "Open History Search"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "96": "icons/icon-96.png"
  }
}
