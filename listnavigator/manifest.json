{
  "manifest_version": 2,
  "name": "List Navigator",
  "version": "0.1.11",
  "description": "Navigate through any webpage with advanced search and filtering",
  "permissions": [
    "activeTab",
    "<all_urls>"
  ],
  "browser_action": {
    "default_title": "Toggle List Navigator",
    "default_icon": {
      "48": "icon48.png"
    }
  },
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["styles.css"],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "48": "icon48.png"
  }
}
